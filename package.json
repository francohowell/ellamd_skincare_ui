{
  "name": "ellamd-ui",
  "version": "0.5.0",
  "description": "The EllaMD application UI.",
  "scripts": {
    "dev": "yarn run start-development",
    "prod": "yarn run start-production",
    "dist": "yarn run build-for-distribution",
    "tcm": "yarn run create-css-ts-definitions",
    "test": "jest",
    "start-development": "yarn run create-css-ts-definitions && yarn run start-development-server",
    "start-production": "yarn run build-for-distribution && yarn run start-production-server",
    "start-development-server": "webpack-dev-server --progress --colors --hot --inline --host 0.0.0.0 --config webpack/webpack.development.config.js",
    "start-production-server": "node build/server",
    "remove-previous-build": "rimraf build/*",
    "build-for-distribution": "yarn run remove-previous-build && yarn run create-css-ts-definitions && yarn run build-webpack",
    "build-webpack": "webpack --progress --optimize-minimize --config webpack/webpack.production.config.js",
    "create-css-ts-definitions": "tcm --pattern '{components,entries,utilities}/**/*.css' --camelCase",
    "heroku-postbuild": "yarn run build-for-distribution",
    "prettier": "yarn run prettier-js && yarn run prettier-ts",
    "prettier-js": "prettier --print-width 100 --trailing-comma es5 --no-bracket-spacing --write --parser babylon \"!(build)/**/*.{js,jsx}\"",
    "prettier-ts": "prettier --print-width 100 --trailing-comma es5 --no-bracket-spacing --write --parser typescript \"!(build)/**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamkha/ellamd-ui.git"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleDirectories": [
      "node_modules",
      "./"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules",
      "\\.(svg)$": "<rootDir>/utilities/empty-module.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "author": "EllaMD",
  "license": "UNLICENSED",
